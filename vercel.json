{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/react"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.html"
      },
      {
        "src": "/assets/(.*)",
        "dest": "public/assets/$1"
      }
    ]
  }
  